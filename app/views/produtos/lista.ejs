<html>
    <body>
        <table>
            <tr>
                <th>id</th>
                <th>nome</th>
                <th>descrição</th>
                <th>preco</th>
            </tr>
            <% for(var i=0; i< lista.length; i++) { %>
                <tr>
                    <td><%= lista[i].id %></td>
                    <td><%= lista[i].nome %></td>
                    <td><%= lista[i].descricao %></td>
                    <td><%= lista[i].preco %></td>                    
                </tr>
            <% } %>
            
        </table>
    </body>
</html>